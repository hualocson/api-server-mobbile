{
  "name": "server-api",
  "version": "1.0.0",
  "description": "Api for mobile",
  "main": "src/index.js",
  "scripts": {
    "generate": "npx prisma generate",
    "prod:swagger-autogen": "node ./build/api/docs/swagger.js",
    "dev:swagger-autogen": "node ./src/api/docs/swagger.js",
    "clean": "rm -rf build && mkdir build ",
    "clean:test": "rm -rf dist_test && mkdir dist_test ",
    "test": "npm run clean:test && babel ./tests -d ./dist_test/tests &&  babel ./src -d ./dist_test/src && node ./dist_test/tests/index.test.js",
    "build-babel": "babel ./src -d ./build",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && npm run prod:swagger-autogen && node ./build/index.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/index.js"
  },
  "keywords": [],
  "author": "HuaLocSon",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prettier-plugin-prisma": "^4.12.0",
    "prisma": "^4.12.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@prisma/client": "^4.12.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.36.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.8.2"
  }
}
